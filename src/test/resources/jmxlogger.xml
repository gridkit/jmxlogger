<mbean-appender>
    <patterns>
    <![CDATA[
USERNAME [a-zA-Z0-9_-]+ 
USER %{USERNAME}
INT (?:[+-]?(?:[0-9]+))
BASE10NUM (?<![0-9.+-])(?>[+-]?(?:(?:[0-9]+(?:\.[0-9]+)?)|(?:\.[0-9]+)))
NUMBER (?:%{BASE10NUM})
BASE16NUM (?<![0-9A-Fa-f])(?:[+-]?(?:0x)?(?:[0-9A-Fa-f]+))
BASE16FLOAT \b(?<![0-9A-Fa-f.])(?:[+-]?(?:0x)?(?:(?:[0-9A-Fa-f]+(?:\.[0-9A-Fa-f]*)?)|(?:\.[0-9A-Fa-f]+)))\b
POSINT \b(?:[1-9][0-9]*)\b
NONNEGINT \b(?:[0-9]+)\b
WORD (\w+)
    ]]> 
    </patterns>
    <match> 
        <pattern>.*[.]%{WORD:name} - %{NUMBER:time}ms</pattern>
        <var name="NAME">name</var>
        <var name="TIME">time</var> 
        <mbean>
            <name>CustomBean:id=%{NAME}</name> 
            <report>TIME</report> 
        </mbean>
    </match>
</mbean-appender>
